#: src/cdlabel.c:434
msgid "Cover label options"
msgstr "Cover-Label-Optionen"

#: src/cdlabel.c:445
msgid "Category:     "
msgstr "Kategorie:   "

#: src/cdlabel.c:457
msgid "Sub-category:"
msgstr "Unterkategorie:"

#: src/cdlabel.c:469
msgid "No date"
msgstr "Kein Datum"

#: src/cdlabel.c:474
msgid "Don't print date on cover"
msgstr "Kein Datum auf das Cover drucken"

#: src/cdlabel.c:476
